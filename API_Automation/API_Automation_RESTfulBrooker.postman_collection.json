							"});\r",
							"\r",
							"//Check the Response time:\r",
							"pm.test(\"TCS-3: Verify the response time is less than 2 second\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(2000);\r",
							"});\r",
							"\r",
							"//Save the Token ID for the next API chains:\r",
							"var responseBody = pm.response.json();\r",
